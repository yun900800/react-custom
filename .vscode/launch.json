    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\sicp\\data-abstract\\data-abstract.js"
        },
        {
            "name": "Debug Jest Tests",
            "type": "node",
            "request": "launch",
            "args": [
                "${workspaceFolder}/node_modules/jest/bin/jest.js",
                "--runInBand",
                "--no-cache",
                "--watch",
                "--debug"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}